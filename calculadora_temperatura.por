programa {
  funcao inicio() {

    //faça um algoritimo que receba temperatura em celsius e converta para fahrenheit

    //1 declarar as variaveis
    inteiro c, f

    //entradas
    escreva("informe a temperatura em ° Celsius para conversão em Fahrenheit: ")
    leia(c)

    //2 processamento 
    f =(9*c+160)/5

    //3 saidas
    escreva("temperatura em Fahrenheit: " + f)



  }
}
